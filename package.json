{
  "name": "tiny-blog",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "generate:css": "npx tailwindcss -i ./tailwind.css -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production remix-serve build"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^5.0.5",
    "@editorjs/code": "^2.8.0",
    "@editorjs/editorjs": "^2.26.4",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/link": "^2.5.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/simple-image": "^1.5.1",
    "@editorjs/table": "^2.2.0",
    "@remix-run/node": "^1.4.1",
    "@remix-run/react": "^1.4.1",
    "@remix-run/vercel": "^1.4.1",
    "ckeditor5-custom-build": "file:ckeditor5",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "moment": "^2.29.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-editor-js": "^2.1.0",
    "react-icons": "^4.3.1",
    "remix-utils": "^5.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.1",
    "@remix-run/eslint-config": "^1.4.1",
    "@remix-run/serve": "^1.4.1",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
