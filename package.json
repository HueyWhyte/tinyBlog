    "@remix-run/vercel": "^1.10.0",
    "@supabase/supabase-js": "^2.2.3",
    "@vercel/node": "^2.6.2",
    "ckeditor5-custom-build": "file:ckeditor5",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "remix-utils": "^5.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.10.0",
    "@remix-run/eslint-config": "^1.10.0",
    "@remix-run/serve": "^1.10.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
